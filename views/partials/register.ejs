<div style="z-index:12;
    display: block;
    text-align: right;
    background-color: #333a40;
    margin-right: 0px;
    padding-right: 20px;
    color: gray;
    position: absolute;
    width: 155px;
    margin-top: 150px;
    padding-top: 400px;
    right: 0px;
    border-radius: 10px;
    padding: 11px;
    font-size: 12px;">

    <form action="/register" method="POST">
        <label style="color:white;">REGISTER</label>
        <input type="text" name="name" placeholder="name">
        <input type="text" name="address" placeholder="address">
        <input type="text" name="city" placeholder="city">
        <input type="text" name="state" placeholder="state">
        <input type="text" name="zipcode" placeholder="zipcode">
    <!-- age(dropdown)-->
        
        <label for="age" style="color:white;">Age</label><br>
        <select name="age" id="lang">
            <option value="">Select Age Range</option>
            <option value="17-21">17 - 21</option>
            <option value="22-30">22 - 30</option>
            <option value="31-38">31 - 38</option>
            <option value="39-44">39 - 44</option>
            <option value="45-60">45 - 60</option>
        </select>
            
    <!--gender (radio)-->
        <label style="color:white;">Gender</label><br>
        <div class="form-check">
            <input class="form-check-input" type="radio" name="genderRadio" id="genderRadio1" value="Female">
            <label class="form-check-label" for="genderRadio1">
              Male
            </label>
          </div>
          <div class="form-check">
            <input class="form-check-input" type="radio" name="genderRadio" id="genderRadio2" value="Female" checked>
            <label class="form-check-label" for="genderRadio2">
                Female
              </label>
          </div>
    <!--consent(checkbox)-->
        <label style="color: white;">Consent Box</label>
        <div class="form-check">
            <input  class="form-check-input" type="checkbox" value="true" id="Checked">
            <label class="form-check-label" for="flexCheckDefault">
              
            </label>
          </div>
          
    <!--Bio(text area)-->
        <label class="bio" for="textAreaExample" style="color: white">BIO</label><br>
        <div class="form-outline">
            <textarea class="form-control" id="bio" rows="4"></textarea>
            
          </div>
          <br>
        <input type="submit">

    </form>
</div>